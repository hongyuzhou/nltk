<script language="JavaScript">
var timeID
var fadeStep=50	//渐显速度(ms)
function showTitle(note){
	var obj=document.getElementById('photoTitle')
	obj.style.left=event.x
	obj.style.top=event.y
	obj.innerHTML=note
	clearInterval(timeID)
	timeID=setInterval(fadeIn,5)
}

function fadeIn(){
var obj=document.getElementById('photoTitle')
	if (obj.filters.Alpha.opacity-fadeStep&lt;100){
		obj.filters.Alpha.opacity+=fadeStep;
	}
}

function hideTitle(){
var obj=document.getElementById('photoTitle')
	clearInterval(timeID)
	obj.filters.Alpha.opacity=0;
}
</script>
